[1mdiff --git a/src/scenes/scene-movie/Scene-movie.jsx b/src/scenes/scene-movie/Scene-movie.jsx[m
[1mnew file mode 100644[m
[1mindex 0000000..8b26002[m
[1m--- /dev/null[m
[1m+++ b/src/scenes/scene-movie/Scene-movie.jsx[m
[36m@@ -0,0 +1,55 @@[m
[32m+[m[32mimport { useEffect, useState } from 'react';[m
[32m+[m[32mimport './Scene-movie.css';[m
[32m+[m
[32m+[m[32mfunction SceneMovie({ sceneData }) {[m
[32m+[m[32m  // Tilamuuttuja elokuvan tiedoille[m
[32m+[m[32m  const [movie, setMovie] = useState(null);[m
[32m+[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    // Funktio, joka hakee elokuvan TMDB API:sta[m
[32m+[m[32m    async function fetchMovie() {[m
[32m+[m[32m      // Tarkistetaan ennen kutsua ett√§ API-avain l√∂ytyy[m
[32m+[m[32m      if (!sceneData.api_key) {[m
[32m+[m[32m        console.error("API key is missing!");[m
[32m+[m[32m        return; // Lopetetaan jos avainta ei ole[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      try {[m
[32m+[m[32m        // Haetaan elokuvadata TMDB API:sta[m
[32m+[m[32m        const response = await fetch(`https://api.themoviedb.org/3/discover/movie?api_key=${sceneData.api_key}&sort_by=popularity.desc&page=1`);[m
[32m+[m[32m        // Muutetaan JSON-muotoom[m[41m [m
[32m+[m[32m        const data = await response.json();[m
[32m+[m
[32m+[m[32m        // Tarkistetaan, ett√§ kutsu palautti elokuvia[m
[32m+[m[32m        if (data.results && data.results.length > 0) {[m
[32m+[m[32m          // Valitaan satunnainen elokuva tuloksista[m
[32m+[m[32m          const randomMovie = data.results[Math.floor(Math.random() * data.results.length)];[m
[32m+[m[32m          // P√§ivitet√§√§n elokuvan tiedot tilamuuttujaan[m
[32m+[m[32m          setMovie(randomMovie);[m
[32m+[m[32m        } else {[m
[32m+[m[32m          console.error("No movies found from TMDB API!");[m
[32m+[m[32m        }[m
[32m+[m[32m      } catch (error) {[m
[32m+[m[32m        console.error('Error fetching movie:', error);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Kutsutaan funktiota heti kun komponentti on ladattu[m
[32m+[m[32m    fetchMovie();[m
[32m+[m[32m  }, [sceneData?.api_key]); //useEffect suoritetaan aina, kun sceneData.api_key muuttuu[m
[32m+[m
[32m+[m[32m  if (!movie) return <p>Ladataan elokuvaa...</p>;[m
[32m+[m
[32m+[m[32m  // Kun elokuvan tiedot on haettu render√∂id√§√§n ruudulle[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div className="scene-movie">[m
[32m+[m[32m      <div className="movie-name">[m
[32m+[m[32m        <h1>{movie.title}</h1>[m
[32m+[m[32m      </div>[m
[32m+[m[32m      <p>{movie.overview}</p>[m
[32m+[m[32m      <img src={`https://image.tmdb.org/t/p/w500${movie.poster_path}`} alt={movie.title} />[m
[32m+[m[32m      </div>[m
[32m+[m[32m  );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport default SceneMovie;[m
