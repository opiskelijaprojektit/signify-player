[32m+[m[32mexport async function fetchMovie(apiKey) {[m
[32m+[m[32m    if (!apiKey) {[m
[32m+[m[32m      console.error("API key is missing!");[m
[32m+[m[32m      return null;[m
[32m+[m[32m    }[m
[32m+[m[41m  [m
[32m+[m[32m    try {[m
[32m+[m[32m      const response = await fetch(`https://api.themoviedb.org/3/discover/movie?api_key=${apiKey}&sort_by=popularity.desc&page=1`);[m
[32m+[m[32m      const data = await response.json();[m
[32m+[m[41m  [m
[32m+[m[32m      if (data.results && data.results.length > 0) {[m
[32m+[m[32m        // Valitaan satunnainen elokuva tuloksista[m
[32m+[m[32m        return data.results[Math.floor(Math.random() * data.results.length)];[m
[32m+[m[32m      } else {[m
[32m+[m[32m        console.error("No movies found from TMDB API!");[m
[32m+[m[32m        return null;[m
[32m+[m[32m      }[m
[32m+[m[32m    } catch (error) {[m
[32m+[m[32m      console.error("Error fetching movie:", error);[m
[32m+[m[32m      return null;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
